<?xml version="1.0"?>
<config>
    <modules>
        <TadashiShoji_ShippingModule>
            <version>0.1.0</version>
            <depends>
                <Mage_Shipping />
            </depends>
        </TadashiShoji_ShippingModule>
    </modules>
    <global>
        <models>
            <shippingmodule>
                <class>TadashiShoji_ShippingModule_Model</class>
            </shippingmodule>
        </models>

        <resources>
            <shippingmodule_setup>
                <setup>
                    <module>TadashiShoji_ShippingModule</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </shippingmodule_setup>
        </resources>
    </global>
    <default>
        <carriers>
            <shippingmodule>
                <active>1</active>
                <model>ShippingModule/carrier_carrier</model>
            </shippingmodule>
        </carriers>
    </default>
</config>
